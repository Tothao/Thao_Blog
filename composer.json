{
    "name": "thao/module-blog",
    "description": "Magento 2 Blog Module",
    "type": "magento2-module",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Thao",
            "email": "tothao2502@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Thao\\Blog\\": ""
        }
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        "thao/module-base": {
            "type": "git",
            "url": "https://github.com/Tothao/Thao_Base.git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "thao/module-base": "dev-master"
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true
        }
    }
}
